<?xml version='1.0' encoding='UTF-8'?>
<types:mule xmlns:types="http://www.mulesoft.org/schema/mule/types">
  <types:catalog>
    <types:type name="bookt_ticket_input" format="xml">
      <types:example format="xml" element="{http://schemas.xmlsoap.org/soap/envelope/}Envelope" location="examples/book_ticket_input.xml"/>
    </types:type>
    <types:type name="print_ticket_Input" format="xml">
      <types:example format="xml" element="{http://schemas.xmlsoap.org/soap/envelope/}Envelope" location="examples/print_ticket_Input.xml"/>
    </types:type>
    <types:type name="print_ticket_process_in" format="json">
      <types:shape format="weave" example="examples/print_ticket_process_in.json"><![CDATA[%dw 2.0

type print_ticket_process_in = {|
  "mail-id": String, 
  "Order-id": String
|} {"example": "{\\\"mail-id\\\":\\\"\\\",\\\"Order-id\\\":\\\"\\\"}"}

]]></types:shape>
    </types:type>
    <types:type name="book_ticket_process_in" format="json">
      <types:shape format="weave" example="examples/book_ticket_process_in.json"><![CDATA[%dw 2.0

type book_ticket_process_in = {|
  OrderID: String, 
  date: String, 
  theaterName: String, 
  movieName: String, 
  show: String, 
  noOfSeats: Number {"typeId": "int"}, 
  price: Number {"typeId": "int"}, 
  address: String, 
  coupen: String, 
  SeatNum: String, 
  emailID: String, 
  Discountedprice: String
|} {"example": "{\\\"OrderID\\\":\\\"\\\",\\\"date\\\":\\\"\\\",\\\"theaterName\\\":\\\"\\\",\\\"movieName\\\":\\\"\\\",\\\"show\\\":\\\"\\\",\\\"noOfSeats\\\":3,\\\"price\\\":200,\\\"address\\\":\\\"\\\",\\\"coupen\\\":\\\"\\\",\\\"SeatNum\\\":\\\"\\\",\\\"emailID\\\":\\\"\\\",\\\"Discountedprice\\\":\\\"\\\"}"}

]]></types:shape>
    </types:type>
  </types:catalog>
  <types:enrichment select="#a9e4219f-9dd2-4789-a7a0-44ee6b034d68">
    <types:processor-declaration>
      <types:input-event>
        <types:message>
          <types:payload type="bookt_ticket_input"/>
        </types:message>
      </types:input-event>
      <types:output-event>
        <types:message>
          <types:payload type="book_ticket_process_in"/>
        </types:message>
      </types:output-event>
    </types:processor-declaration>
  </types:enrichment>
  <types:enrichment select="#f2805a15-6d35-41c9-ad3b-0ba65010ca2f">
    <types:processor-declaration>
      <types:input-event>
        <types:message>
          <types:payload type="print_ticket_Input"/>
        </types:message>
      </types:input-event>
      <types:output-event>
        <types:message>
          <types:payload type="print_ticket_process_in"/>
        </types:message>
      </types:output-event>
    </types:processor-declaration>
  </types:enrichment>
</types:mule>